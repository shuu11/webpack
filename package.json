    "status":"git status",                          "#1-exp":"git status",                                        "#1-memo":"作業ツリー内の差分ファイルを表示",
    "mv":"git mv",                                  "#1-exp":"git mv",                                            "#1-memo":"ファイルを移動/ファイル名を変更",
